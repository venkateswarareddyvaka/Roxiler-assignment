{
    "name": "your-project-name",
    "version": 2,
    "builds": [
      { 
        "src": "server.js", 
        "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/server.js" },
      { "src": "/api/transactions", "dest": "/server.js" },
      { "src": "/api/statistics", "dest": "/server.js" },
      { "src": "/api/batcharts", "dest": "/server.js" }
    ]
  }
  